{% extends "base.html.twig" %}

{% block content %}
    <h1>Catalog</h1>
    <ul class="thumbnails">
        {% for product in products %}
        <li class="span3">
            <div class="thumbnail">
                <img data-src="" alt="300x200" style="width: 300px; height: 200px;" src="data:image/png;base64,">
                <div class="caption">
                    <h3>{{ product['title'] }}</h3>
                    <p>price: {{ product['price'] }}</p>
                    <p>Description...</p>
                    <form action="/cart" method="POST">
                        <button name="product" value="{{ product['id'] }}">Add to cart</button>
                    </form>
                    <p><a href="/catalog/{{ product['id'] }}" class="btn">Detail</a></p>
                </div>
            </div>
        </li>
        {% endfor %}
    </ul>
{% endblock %}